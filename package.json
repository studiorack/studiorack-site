{
  "name": "studiorack-site",
  "version": "2.0.0",
  "description": "Audio plugin site with searchable plugin list, tools and documentation",
  "type": "module",
  "main": "./main.js",
  "exports": "./build/main.js",
  "types": "./build/main.d.ts",
  "files": [
    "build"
  ],
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "dev:static": "serve ./out",
    "format": "prettier . --write",
    "lint": "eslint ./**/*.ts",
    "start": "next start",
    "test": "vitest run ./tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/studiorack/studiorack-site.git"
  },
  "keywords": [
    "audio",
    "plugin",
    "site",
    "vst",
    "daw",
    "metadata",
    "search"
  ],
  "author": "kmturley",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/studiorack/studiorack-site/issues"
  },
  "homepage": "https://github.com/studiorack/studiorack-site#readme",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@studiorack/core": "^2.0.12",
    "gray-matter": "^4.0.3",
    "next": "^14.2.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "remark": "^15.0.1",
    "remark-html": "^16.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.2.0",
    "@types/node": "20.14.8",
    "@types/react": "18.3.3",
    "eslint": "^8.57.0",
    "globals": "^15.2.0",
    "prettier": "^3.2.5",
    "serve": "^14.2.3",
    "tsx": "^4.10.1",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.8.0",
    "vitest": "^1.6.0"
  }
}
